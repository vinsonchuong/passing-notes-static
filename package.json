{
  "name": "passing-notes-static",
  "version": "0.0.0",
  "description": "A static file middleware for passing-notes",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/passing-notes-static",
  "bugs": "https://github.com/vinsonchuong/passing-notes-static/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/passing-notes-static",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "dependencies": {
    "content-type": "^1.0.4",
    "fresh": "^0.5.2",
    "mime": "^2.4.7",
    "url-parse": "^1.4.7"
  },
  "peerDependencies": {
    "passing-notes": "^6.2.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "ava-patterns": "^2.4.1",
    "passing-notes": "^6.3.1",
    "puppet-strings": "^4.0.1",
    "puppet-strings-chrome": "^2.0.1",
    "semantic-release": "^17.3.1",
    "xo": "^0.37.1"
  },
  "ava": {
    "verbose": true,
    "timeout": "3m"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "import/no-anonymous-default-export": "off"
    }
  }
}
