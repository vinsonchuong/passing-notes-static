{
  "name": "passing-notes-static",
  "version": "0.0.0",
  "description": "A static file middleware for passing-notes",
  "keywords": [],
  "homepage": "https://github.com/vinsonchuong/passing-notes-static",
  "bugs": "https://github.com/vinsonchuong/passing-notes-static/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/passing-notes-static",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "type": "module",
  "main": "index.js",
  "dependencies": {
    "@tinyhttp/etag": "^2.1.0",
    "content-type": "^1.0.5",
    "fresh": "^0.5.2",
    "mime": "^3.0.0",
    "strip-indent": "^4.0.0",
    "url-parse": "^1.5.10"
  },
  "peerDependencies": {
    "passing-notes": "^6.2.0"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "ava-patterns": "^3.2.1",
    "passing-notes": "^6.6.1",
    "puppet-strings": "^5.0.1",
    "puppet-strings-chrome": "^2.0.3",
    "semantic-release": "^21.0.7",
    "xo": "^0.55.1"
  },
  "ava": {
    "verbose": true,
    "timeout": "3m"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "no-else-return": "off",
      "import/no-anonymous-default-export": "off",
      "unicorn/text-encoding-identifier-case": "off"
    }
  },
  "packageManager": "yarn@3.2.1"
}
